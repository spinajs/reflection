{
  "extends": [
    "tslint-circular-dependencies","tslint:recommended", "tslint-config-prettier","tslint-no-unused-expression-chai"
  ],
  "rules":{
    "max-classes-per-file": {
      "severity": "off"
    },
    "no-instanceof-operator": {
      "severity": "off"
    },
    "imports-after-export": {
      "severity": "off"
    },
    "no-console": {
      "severity": "off"
    },
    "object-literal-sort-keys": {
      "severity": "off"
    },
    "ordered-imports": {
      "severity": "off"
    }
  }
}